\begin_layout Subsubsection
Lanczos interpolation
\end_layout

\begin_layout Standard
Lanczos's weighting function isgiven by,
\begin_inset Formula 
\[
W_{A}\left(x\right)=\begin{cases}
1 & x=0\\
\mathrm{sinc}\left(\pi x\right)\mathrm{sinc}\left(\frac{\pi x}{A}\right) & \left|x\right|<A\\
0 & \left|x\right|>A
\end{cases}
\]

\end_inset

Lanczos interpolations need (
\begin_inset Formula $2A$
\end_inset

)x(
\begin_inset Formula $2A$
\end_inset

) sized kernel.
\end_layout

\begin_layout Standard
Lanczos3
\end_layout

\begin_layout Standard
for 
\begin_inset Formula $\alpha=0$
\end_inset


\begin_inset Formula 
\begin{align*}
w_{0} & =W\left(\alpha+2\right) &  &  & w_{3} & =W\left(-1\right)\\
w_{1} & =W\left(\alpha+1\right) &  &  & w_{4} & =W\left(-2\right)\\
w_{2} & =W\left(0\right) &  &  & w_{5} & =W\left(-3\right)
\end{align*}

\end_inset


\begin_inset Formula 
\[
w_{1}=W\left(\alpha+1\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{w_{1}}{w_{0}+w_{1}}=\frac{\mathrm{sinc}\left(\pi\left(\alpha+1\right)\right)\mathrm{sinc}\left(\frac{\pi\left(\alpha+1\right)}{3}\right)}{\mathrm{sinc}\left(\pi\left(\alpha+2\right)\right)\mathrm{sinc}\left(\frac{\pi\left(\alpha+2\right)}{3}\right)+\mathrm{sinc}\left(\pi\left(\alpha+1\right)\right)\mathrm{sinc}\left(\frac{\pi\left(\alpha+1\right)}{3}\right)}
\]

\end_inset


\begin_inset Formula 
\[
h_{0}^{\alpha}=2-\frac{w_{1}^{\alpha}}{w_{0}^{\alpha}+w_{1}^{\alpha}},\quad h_{1}^{\alpha}=\alpha-\frac{w_{3}^{\alpha}}{w_{2}^{\alpha}+w_{3}^{\alpha}},\quad h_{2}^{\alpha}=2+\frac{w_{5}^{\alpha}}{w_{4}^{\alpha}+w_{5}^{\alpha}}
\]

\end_inset


\begin_inset Formula 
\begin{align*}
w_{0}\left(x\right) & =W\left(x+2\right) &  &  & w_{3}\left(x\right) & =W\left(x-1\right)\\
w_{1}\left(x\right) & =W\left(x+1\right) &  &  & w_{4}\left(x\right) & =W\left(x-2\right)\\
w_{2}\left(x\right) & =W\left(x\right) &  &  & w_{5}\left(x\right) & =W\left(x-3\right)
\end{align*}

\end_inset


\begin_inset Formula 
\[
h_{0}^{\alpha}=2+\alpha-\frac{w_{1}^{\alpha}}{w_{0}^{\alpha}+w_{1}^{\alpha}},\quad h_{1}^{\alpha}=\alpha-\frac{w_{3}^{\alpha}}{w_{2}^{\alpha}+w_{3}^{\alpha}},\quad h_{2}^{\alpha}=2-\alpha+\frac{w_{5}^{\alpha}}{w_{4}^{\alpha}+w_{5}^{\alpha}}
\]

\end_inset


