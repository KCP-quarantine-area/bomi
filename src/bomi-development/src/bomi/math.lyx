\end_inset


\begin_inset Formula 
\begin{align*}
w_{0} & =w_{2}=W\left(1\right)=0\\
w_{1} & =W\left(0\right)=1\\
w_{3} & =W\left(-2\right)=0
\end{align*}

\end_inset


\begin_inset Formula 
\[
h_{0}^{\alpha}=0,\quad h_{1}^{\alpha}=1
\]

\end_inset


\begin_inset Formula 
\[
h_{0}^{\alpha}=1+\alpha-\frac{w_{1}^{\alpha}}{w_{0}^{\alpha}+w_{1}^{\alpha}},\quad h_{1}^{\alpha}=1-\alpha+\frac{-\frac{1}{3}x^{3}+\frac{4}{5}x^{2}-\frac{7}{15}x}{x^{3}-\frac{1}{3}x^{3}-\frac{9}{5}x^{2}-\frac{1}{5}x+1.0+\frac{4}{5}x^{2}-\frac{7}{15}x}
\]

\end_inset

for 
\begin_inset Formula $\alpha=1$
\end_inset


\begin_inset Formula 
\begin{align*}
w_{0} & =W\left(2\right)=0 &  &  & w_{2} & =W\left(0\right)=1\\
w_{1} & =w_{3}=W\left(1\right) & =0
\end{align*}

\end_inset


\begin_inset Formula 
\[
h_{0}^{\alpha}=1,\quad h_{1}^{\alpha}=0
\]

\end_inset


\begin_inset Formula 
\[
h_{0}^{\alpha}=1+\alpha-\frac{w_{1}^{\alpha}}{w_{0}^{\alpha}+w_{1}^{\alpha}},\quad h_{1}^{\alpha}=1-\alpha+\frac{w_{3}^{\alpha}}{w_{2}^{\alpha}+w_{3}^{\alpha}}
\]

\end_inset


